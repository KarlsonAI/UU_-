first = int(input("Введите число: "))
second = int(input("Введите число: "))
third = int(input("Введите число: "))
if first == second == third:
    print(3)
elif first != second != third !=first:
    print(0)
else:           # или вместо "else" прописать условия под # ниже
    print(2)

